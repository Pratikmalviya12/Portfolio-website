# Portfolio Website Environment Variables
# Copy this file to .env and update with your actual values

# API Configuration
REACT_APP_API_BASE_URL=https://your-api-domain.com/api
REACT_APP_USE_MOCK_DATA=true

# Contact Form Configuration (if using external service)
REACT_APP_CONTACT_API_URL=https://your-contact-api.com/submit

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Error Tracking (optional)
REACT_APP_SENTRY_DSN=https://your-sentry-dsn.com

# Build Configuration
GENERATE_SOURCEMAP=false

# Legacy Vite Configuration (if migrating)
# VITE_API_ENDPOINT=http://localhost:8000
# VITE_CONTACT_EMAIL=your.email@example.com
